<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"	>

		<bean id="customer" class="springassignmentQ1.Customer">
                <property name = "customerId" value="101"></property>
                <property name = "customerName" value="Akash"></property>
                <property name = "customerContact" value="543125"></property>
                <property name="customerAddress" ref="address1"></property>
        
        </bean>
        
        <bean id="address1" class="springassignmentQ1.Address">
                <property name="street" value="M.G. Rd"></property>
                <property name="city" value="Kolkata"></property>
                <property name="state" value="West Bengal"></property>
                <property name="zip" value="700001"></property>
                <property name="country" value="India"></property>
        </bean>

</beans>